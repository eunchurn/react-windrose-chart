    "minimatch": "^3.0.4",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react": "^16.10.1",
    "react-cache": "^2.0.0-alpha.1",
    "react-dom": "^16.10.1",
    "recompose": "^0.30.0",
    "rimraf": "^3.0.0",
    "rollup": "1.25.2",
    "rollup-plugin-alias": "^2.0.1",
